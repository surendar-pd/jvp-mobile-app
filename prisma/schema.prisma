  theme_light_overrides                                              Json?
  default_theme_dark                                                 String?           @db.VarChar(255)
  theme_dark_overrides                                               Json?
  report_error_url                                                   String?           @db.VarChar(255)
  report_bug_url                                                     String?           @db.VarChar(255)
